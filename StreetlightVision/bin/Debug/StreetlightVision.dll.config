<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- Browser should be: Chrome, IE, FF -->
    <add key="BROWSER" value="Chrome" />
    <!--This is timeout in millisecond of driver wait timeout-->
    <add key="DRIVER_WAIT_TIMEOUT" value="60000" />
    <!--This is timeout in millisecond of retrying to locate elements in POM -->
    <add key="LOCATOR_RETRYING_TIMEOUT" value="5000" />
    <!--This is polling interval-->
    <add key="LOCATOR_POLLING_INTERVAL" value="200" />
    <!--This is time wait for alarm trigger/auto acknowledged (seconds)-->
    <add key="ALARM_TIME_WAIT" value="180" />
    <!--This is test case Retry Count if error -->
    <add key="RETRY_COUNT" value="2" />
    <!--This is SLV Host Name -->
    <add key="HOST_NAME" value="slvci-01.eng.ssnsgs.net" />
    <!--This is SLV URL -->
    <add key="SLV_URL" value="https://slvci-01.eng.ssnsgs.net:8443/reports/" />
    <!--This is SLV HTTPS URL-->
    <add key="SLV_HTTPS_URL" value="https://slvci-01.eng.ssnsgs.net:8443/reports/" />
    <!--This is TimeZone of SLV Server-->
    <add key="SERVER_TIMEZONE" value="UTC" />
    <!--The root geozone's name-->
    <add key="ROOT_GEOZONE_NAME" value="GeoZones" />
    <!--This is to turn ON or OFF writing test details-->
    <add key="STEP_LOGGING_ENABLED" value="ON" />
    <!--This is to turn ON or OFF writing SUCCESS or FAIL message in Verify methods-->
    <add key="VERIFY_LOGGING_ENABLED" value="ON" />
    <!--This is to find out which items with name matching the pattern can be deleted by a script-->
    <add key="DELETABLE_NAME_PATTERN" value="(.*\d{19}.*)|(New GeoZone \d{1,})" />
    <add key="DELETE_IN_GEOZONES" value="Automation;Free Devices;New GeoZone;New Install;Installation;Verification" />
    <add key="FIREFOX_BINARY_PATH" value="C:\Program Files (x86)\Mozilla Firefox\firefox.exe" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
</configuration>
